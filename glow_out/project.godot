; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="glow_out"
config/tags=PackedStringArray("2d", "gdscript", "personal")
run/main_scene="res://scenes/menus/main_menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
run/max_fps=120
boot_splash/show_image=false
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://resources/default_bus_layout.tres"

[autoload]

GameManager="*res://scripts/autoloads/game_manager.gd"
Globals="*res://scripts/autoloads/globals.gd"
SceneLoader="*res://scripts/autoloads/scene_loader.gd"
UISoundPlayer="*res://scenes/ui_sound_player.tscn"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=4
window/size/resizable=false
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"

[dotnet]

project/assembly_name="mini_jam_160_platformer"

[editor]

export/convert_text_resources_to_binary=false
version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[file_customization]

folder_colors={
"res://addons/": "red",
"res://addons/godot-git-plugin/": "purple",
"res://addons/kenney_assets/": "green",
"res://addons/sounds/": "orange",
"res://resources/": "green",
"res://scenes/": "orange",
"res://scenes/gameplay_elements/": "pink",
"res://scenes/levels/": "green",
"res://scenes/menus/": "purple",
"res://scenes/one_way_wall/": "blue",
"res://scripts/": "yellow",
"res://scripts/autoloads/": "purple",
"res://scripts/components/": "pink",
"res://scripts/gameplay_elements/": "blue",
"res://scripts/menus/": "gray"
}

[global_group]

rooms=""
exits=""
springboards=""

[gui]

theme/custom_font="res://addons/kenney_assets/Kenney Mini.ttf"

